<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app 
   xmlns="http://java.sun.com/xml/ns/javaee" 
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" 
   version="2.5">
  <servlet>
    <servlet-name>ScanPassage</servlet-name>
    <servlet-class>main.java.service.ScanPassage</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GetCours</servlet-name>
    <servlet-class>main.java.service.GetCours</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CoAdmin</servlet-name>
    <servlet-class>main.java.service.CoAdmin</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GetEtud</servlet-name>
    <servlet-class>main.java.service.GetStudents</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AddEtud</servlet-name>
    <servlet-class>main.java.service.AddStudent</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AddCour</servlet-name>
    <servlet-class>main.java.service.AddCour</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AddPresence</servlet-name>
    <servlet-class>main.java.service.AddPresence</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RmEtud</servlet-name>
    <servlet-class>main.java.service.RmStudent</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RmCour</servlet-name>
    <servlet-class>main.java.service.RmCour</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UpdEtud</servlet-name>
    <servlet-class>main.java.service.UpdStudent</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UpdCour</servlet-name>
    <servlet-class>main.java.service.UpdCour</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ScanPassage</servlet-name>
    <url-pattern>/scan</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GetEtud</servlet-name>
    <url-pattern>/etudiants</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GetCours</servlet-name>
    <url-pattern>/cours</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CoAdmin</servlet-name>
    <url-pattern>/co_admin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AddEtud</servlet-name>
    <url-pattern>/add_etud</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AddCour</servlet-name>
    <url-pattern>/add_cour</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AddPresence</servlet-name>
    <url-pattern>/add_presence</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RmEtud</servlet-name>
    <url-pattern>/rm_etud</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UpdEtud</servlet-name>
    <url-pattern>/upd_etud</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RmCour</servlet-name>
    <url-pattern>/rm_cour</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UpdCour</servlet-name>
    <url-pattern>/upd_cour</url-pattern>
  </servlet-mapping>
</web-app>
