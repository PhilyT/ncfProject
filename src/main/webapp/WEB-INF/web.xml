<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app 
   xmlns="http://java.sun.com/xml/ns/javaee" 
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" 
   version="2.5">
  <servlet>
    <servlet-name>ScanPassage</servlet-name>
    <servlet-class>main.java.service.ScanPassage</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GetCours</servlet-name>
    <servlet-class>main.java.service.GetCours</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ScanPassage</servlet-name>
    <url-pattern>/scan</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GetCours</servlet-name>
    <url-pattern>/cours</url-pattern>
  </servlet-mapping>
</web-app>
