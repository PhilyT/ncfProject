<% var title = 'Liste des Etudiants' %>
<% include head %>
<div class="container" >
    <div class="row centered-form">
            <div class="col-xs-12 col-sm-12 col-md-6 col-sm-offset-6 col-md-offset-3">
                <div class="panel panel-default">
                    <div class="panel-heading">

                        <h1 class="panel-title"><strong>Liste des Etudiants</strong></h1>
                    </div>
                    <div class="panel-body">
                    <% for(var i = 0; i < etudiants.length; i++) { %>
                        <div class="alert alert-primary" role="alert">   <div class="alert alert-success" role="alert"> <p> Prenom : <b><%= etudiants[i].prenom %>  </b> </p> </div> <div class="alert alert-danger" role="alert"> <p>Nom : <b><%= etudiants[i].nom %> </b></p> </div> <div class="alert alert-info" role="alert"> <p> Numéro ancienne carte : <b><%= etudiants[i].historisation %> </b> </p> </div> <div class="alert alert-warning" role="alert"> <p> Carte étudiante actuel : <b><%= etudiants[i].idCarte %> </b></p> </div> </div> <br />
                    <% } %>
                    <a href="/gestionE"><span class="glyphicon glyphicon-arrow-left"></span> Retour</a>
                    </div>
                </div>
            </div>
        </div>
    </div>

<% include foot%>